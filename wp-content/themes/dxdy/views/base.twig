{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		{# <a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a> #}
    <!-- Google Tag Manager (noscript) -->
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NDD4JMN" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
		{% block header %}
        <div class="wrapper">
            {% if custom_logo_url %}
                    <a class="hdr-logo-link" href="{{site.link}}" rel="home">
                        <img src="{{custom_logo_url}}" alt="{{site.name}}"/>
                    </a>
            {% else %}
                <h1 class="hdr-logo" role="banner">
                    <a class="hdr-logo-link" href="{{site.link}}" rel="home">{{site.name}}</a>
                </h1>
            {% endif %}
            {% include "_components/com-header.twig" with { 'nav-menu' : nav_menu.get_items } %}
        </div>
    {% endblock %}

		<main id="content" role="main" class="content-wrapper {% if template_name == 'template-country' %} country-template {% endif %} ">
      <div class="wrapper {{sidebar_class}}">
        {% block content %}
          Sorry, no content
        {% endblock %}
      </div>
      {# {% if sidebar %}
        <aside class="layout-sidebar">
          {{sidebar}}
        </aside>
      {% endif %}
      {% include '_components/hero.twig' %}
      {% include '_components/highlighted-text.twig' %}
      {% include '_components/two-colgrid.twig' %}
      {% include '_components/three-colgrid.twig' %}
      {% include '_components/call-to-action.twig' %}
      {% include '_components/email-sign-up.twig' %}
      {% include '_components/chart.twig' %} #}
    </main>

		{% block footer %}
      {% include '_components/com-footer.twig'%}
      {{ function('wp_footer') }}
    {% endblock %}

  {# if you need to include template-specific vendor JS as with plugins, it goes here #}
    {% block footerscripts %}
      
    {% endblock %}
	</body>
</html>
